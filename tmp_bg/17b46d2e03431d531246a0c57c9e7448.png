    $(function () {
        $("#Spanalikecodedebug").html('15');
        // trigger event when 'like' link is clicked
        var cookie = $.cookie('mybigcookie' + '605990');

        $("#alikecodedebug").click(function (e) {
            if (cookie == null) {
                insert_like();

                // get current record (parent div)
                var thisrecord = $(this).closest("div");

                // get number of likes for current record and increment by 1
                var likes = "<p class='padR2 fLeft' style='position: relative; top: -16px;'><img src='http://service.tuoitre.vn/TTO-Static/Images/quan-tam.jpg' /></p><span style='padding-left:5px;'>" + (parseInt($("#Spanalikecodedebug").html()) + 1) + "</span>";

                // display new number of likes
                $("#Spanalikecodedebug").html(likes);

                $("#alikecodedebug").remove();

                // $("#alikenone").attr("disabled", true);
                //  var submitObj = $('goButton');
                //	submitObj.disabled = true;

                var previousrecord = "";
                var prevrecordlikes = "";
                do {
                    // get previous record
                    prevrecord = thisrecord.prev("div");

                    // get number of likes for previous record
                    prevrecordlikes = parseInt(prevrecord.find("span").html());

                    // if the number of likes for the current record is less than or equal to that of the previous record break out of the loop immediately
                    if (likes <= prevrecordlikes) break;

                    // insert previous record after current record
                    thisrecord.after(prevrecord);
                }
                // continue until number of likes for previous record exceed that of the current record
                while (likes >= prevrecordlikes);
            }

            // prevent default browser behaviour
            e.preventDefault();
        });
    });        
</script>
